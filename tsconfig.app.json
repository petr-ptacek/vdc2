{
	"extends": "@vue/tsconfig/tsconfig.dom.json",
	"include": [
		"typed-svg-icons.d.ts",
		"env.d.ts",
		"src/**/*",
		"src/**/*.vue"
	],
	"exclude": [
		"src/**/__tests__/*"
	],
	"compilerOptions": {
		"composite": true,
		"tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
		"allowJs": true,
		"baseUrl": ".",
		"paths": {
			"@/*": [
				"./src/*"
			]
		}
	}
}
